<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="zv2.com.cn.entity.usr.visitor.Visitor" table="usr_visitor">
        <id name="id" column="id" type="java.lang.Integer">
            <generator class="native"></generator>
        </id>
        <property name="name"/>
        <property name="ip"/>
        <property name="vipRank"/>
        <property name="gmtCreate"/>
        <property name="gmtModified"/>
        <many-to-one name="room" class="zv2.com.cn.entity.pub.room.Room" column="roomId"/>
    </class>
</hibernate-mapping>